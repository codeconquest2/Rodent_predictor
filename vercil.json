{
  "version": 2,
  "builds": [
    {
      "src": "api/predict.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "50mb" }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/predict.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/predict.py"
    }
  ]
}